<component-set>
	<components>
		<component>
			<role>org.apache.maven.lifecycle.mapping.LifecycleMapping</role>
			<role-hint>exe</role-hint>
			<implementation>
				org.apache.maven.lifecycle.mapping.DefaultLifecycleMapping
			</implementation>
			<configuration>
				<phases>
					<generate-sources>
						com.github.randomcodeorg.netmaven:netmaven:${project.version}:netmavenGenerateSources
					</generate-sources>
					<compile>
						com.github.randomcodeorg.netmaven:netmaven:${project.version}:compile
					</compile>
					<test-compile>
						com.github.randomcodeorg.netmaven:netmaven:${project.version}:compileTest
					</test-compile>
					<install>
						org.apache.maven.plugins:maven-install-plugin:install
					</install>
				</phases>
			</configuration>
		</component>
		<component>
			<role>org.apache.maven.artifact.handler.ArtifactHandler</role>
			<role-hint>exe</role-hint>
			<implementation>
				org.apache.maven.artifact.handler.DefaultArtifactHandler
			</implementation>
			<configuration>
				<extension>exe</extension>
				<type>exe</type>
				<packaging>exe</packaging>
			</configuration>
		</component>
		<component>
			<role>org.apache.maven.lifecycle.mapping.LifecycleMapping</role>
			<role-hint>dll</role-hint>
			<implementation>
				org.apache.maven.lifecycle.mapping.DefaultLifecycleMapping
			</implementation>
			<configuration>
				<phases>
					<generate-sources>
						com.github.randomcodeorg.netmaven:netmaven:${project.version}:netmavenGenerateSources
					</generate-sources>
					<compile>
						com.github.randomcodeorg.netmaven:netmaven:${project.version}:compile
					</compile>
					<test-compile>
						com.github.randomcodeorg.netmaven:netmaven:${project.version}:compileTest
					</test-compile>
					<install>
						org.apache.maven.plugins:maven-install-plugin:install
					</install>
				</phases>
			</configuration>
		</component>
		<component>
			<role>org.apache.maven.artifact.handler.ArtifactHandler</role>
			<role-hint>dll</role-hint>
			<implementation>
				org.apache.maven.artifact.handler.DefaultArtifactHandler
			</implementation>
			<configuration>
				<extension>dll</extension>
				<type>dll</type>
				<packaging>dll</packaging>
			</configuration>
		</component>
		<component>
			<role>org.eclipse.aether.spi.connector.RepositoryConnectorFactory
			</role>
			<implementation>com.github.randomcodeorg.netmaven.netmaven.nuget.NugetRepositoryConnectorFactory
			</implementation>
			<role-hint>nuget</role-hint>
		</component>
	</components>
</component-set>